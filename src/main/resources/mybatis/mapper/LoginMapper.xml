<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.youxin.alumni_management.mapper.LoginMapper">

    <select id="findUserByName" resultType="User" parameterType="String">
        select * from user where userName = #{userName}
    </select>

    <select id="findAdminByName" resultType="Admin" parameterType="String">
        select * from admin where adminName = #{adminName}
    </select>
</mapper>